{
  "consumer": {
    "name": "UsersConsumer"
  },
  "interactions": [
    {
      "description": "GET single user",
      "providerStates": [
        {
          "name": "User with ID 0 exists"
        }
      ],
      "request": {
        "method": "GET",
        "path": "/users/0"
      },
      "response": {
        "body": {
          "firstName": "Petkan",
          "id": 0,
          "lastName": "Petkanov",
          "username": "test.petkan"
        },
        "headers": {
          "Content-Type": "application/json; charset=UTF-8"
        },
        "matchingRules": {
          "body": {
            "$.firstName": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.lastName": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.username": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "header": {
            "Content-Type": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "application/json(;\\s?charset=[\\w\\-]+)?"
                }
              ]
            }
          }
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-jvm": {
      "version": "4.3.6"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "ZooManager"
  }
}
